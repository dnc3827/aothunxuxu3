{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/server.js"
        },
        {
            "src": "/uploads/(.*)",
            "dest": "/public/uploads/$1"
        }
    ]
}